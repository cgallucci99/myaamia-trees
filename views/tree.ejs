                    <a href="/" role="menuitem" > Map </a>
                </li>
                <li role="presentation">
                        <a href="/trees" role="menuitem" > Trees </a>
                    </li>                 
                <li role="presentation">
                    <a href="https://miamioh.edu/myaamia-center/index.html" role="menuitem" > Myaamia Center </a>
                </li>
            </ul>
        </header>
        <div class="clearfix"></div>
        <main>
            <header>
                <h1 class="col-sm-12" style="text-align: center;"><%=tree.english_name%></h1>
            </header>
            <section class="colPrimary col-xs-12">
                <div class="content removepadding col-sm-12">
                    <div class="contentRow row">
                        <section class="col-md-6 defaultSlotStyle">
                            <div class="imageContainer">
                                <img src="<%=tree.img%>" alt="<%=tree.english_name%>" class="noEffect">
                            </div>
                        </section>
                        <section class="col-md-6 defaultSlotStyle">
                            <div>
                                <h6>Myaamia name : <%=tree.myaamia_name%></h6>
                                <h6>Latin name : <%=tree.latin_name%></h6>
                                <p><%=tree.description%></p>
                                <a href="https://www.google.com/maps/search/<%=tree.location%>" class="whiteCalloutButton">Find it on the map</a>
                            </div>
                        </section>
                    </div>
                </div>
            </section>
        </main>
